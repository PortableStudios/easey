  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "plop": "plop",
    "update-theme": "figma2theme generate-chakra -o ./src/theme/_generated",
    "lint": "eslint . --fix --ext .js,.ts,.tsx",
    "typecheck": "tsc -p ./tsconfig.json --noEmit & tsc -p ./cypress/tsconfig.json --noEmit",
    "storybook": "start-storybook -p 6006 -s ./public",
    "build-storybook": "build-storybook -s ./public",
    "screenshots": "rimraf __screenshots__ && storycap http://localhost:6006 --flat",
    "analyze": "cross-env ANALYZE=true yarn build",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage && open ./coverage/lcov-report/index.html",
    "test:debug": "node --inspect-brk ../../node_modules/jest/bin/jest.js --runInBand",
    "cypress:open": "cypress open",
    "cypress:run": "cypress run"
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn typecheck & lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,ts,tsx}": "eslint --fix"
  },
